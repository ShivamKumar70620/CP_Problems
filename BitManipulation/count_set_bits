code:-

[1]:- set bits in a range

class Solution {
public:
    int bits(int n){
        if(n==0)
            return 0;
        return (n&1) + bits(n>>1);
    }
    vector<int> countBits(int n) {
        vector<int> v;
        for(int i=0;i<=n;i++){
            v.push_back(bits(i));
        }
        return v;
    }
};
